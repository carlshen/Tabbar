<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabBar">
        <attr name="tabPaddingVertical" format="dimension"/>
        <attr name="iconPadding" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="focusColor" format="color"/>
        <attr name="normalColor" format="color"/>
        <attr name="tabClickBackground">
            <enum name="ripple" value="0"/>
            <enum name="rippleOutside" value="1"/>
            <enum name="gray" value="2"/>
        </attr>
    </declare-styleable>
</resources>